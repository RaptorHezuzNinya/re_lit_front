  "private": true,
  "scripts": {
    "start": "polymer serve",
    "start:webpack": "webpack-dev-server  --disableHostCheck=true --color --open --mode development",
    "build": "polymer build --auto-base-path && gulp prpl-server",
    "build:static": "polymer build",
    "build:webpack": "rm -rf dist && webpack --config webpack.prod.js",
    "serve": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es5-bundled",
    "serve:webpack": "polymer serve --port 3000 dist",
    "test": "karma start --coverage",
    "test:watch": "karma start --auto-watch=true --single-run=false",
    "test:update-snapshots": "karma start --update-snapshots",
    "test:prune-snapshots": "karma start --prune-snapshots",
    "test:legacy": "karma start --legacy --coverage",
    "test:legacy:watch": "karma start --legacy --auto-watch=true --single-run=false",
    "test:bs": "karma start karma.bs.config.js --legacy --coverage"
  },
