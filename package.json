{
	"name": "pwa-starter-kit",
	"version": "0.9.0",
	"contributors": [
		"The Polymer Authors"
	],
	"license": "BSD-3-Clause",
	"repository": "Polymer/pwa-starter-kit",
	"scripts": {
		"start": "polymer serve",
		"start:webpack": "webpack-dev-server  --disableHostCheck=true --color --open --mode development",
		"build": "polymer build --auto-base-path && gulp prpl-server",
		"build:static": "polymer build",
		"build:webpack": "rm -rf dist && webpack --config webpack.prod.js",
		"serve": "prpl-server --root server/build",
		"serve:static": "polymer serve --port 5000 build/es5-bundled",
		"serve:webpack": "polymer serve --port 3000 dist",
		"test": "npm run test:unit && npm run test:integration",
		"test:integration": "mocha test/integration --timeout=10000",
		"test:unit": "polymer test --module-resolution=node --npm",
		"test:sauce": "polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
		"test:regenerate_screenshots": "mocha test/integration/screenshots-baseline/regenerate.js --timeout=15000"
	},
	"dependencies": {
		"@material/card": "^3.2.0",
		"@polymer/app-layout": "^3.0.0",
		"@polymer/polymer": "^3.1.0",
		"@webcomponents/webcomponentsjs": "^2.2.4",
		"axios": "^0.18.0",
		"babel-loader": "^8.0.6",
		"css-loader": "^2.1.1",
		"lit-element": "^2.1.0",
		"pwa-helpers": "^0.9.0",
		"redux": "^4.0.0",
		"redux-thunk": "^2.3.0",
		"reselect": "^4.0.0",
		"style-loader": "^0.23.1"
	},
	"devDependencies": {
		"@babel/cli": "^7.4.5",
		"@babel/core": "^7.4.5",
		"@babel/node": "^7.4.5",
		"@babel/preset-env": "^7.4.5",
		"@babel/register": "^7.4.4",
		"eslint": "^5.12.0",
		"eslint-config-prettier": "^3.5.0",
		"eslint-plugin-import": "^2.16.0",
		"eslint-plugin-lit": "^1.0.0",
		"eslint-plugin-prettier": "^3.0.1",
		"prettier": "^1.15.3",
		"prettier-plugin-html-template-literals": "^1.0.5",
		"webpack": "^4.29.5",
		"webpack-cli": "^3.2.3",
		"webpack-dev-server": "^3.2.0",
		"webpack-merge": "4.2.1",
		"html-webpack-plugin": "^3.2.0",
		"copy-webpack-plugin": "^5.0.0",
		"sass-loader": "^7.1.0",
		"file-loader": "^4.0.0",
		"node-sass": "^4.12.0",
		"gulp": "^4.0.0",
		"gulp-rename": "^1.3.0",
		"gulp-replace": "^1.0.0",
		"del": "^3.0.0",
		"prpl-server": "^1.4.0",
		"polyserve": "^0.27.0",
		"redux-logger": "^3.0.6",
		"workbox-webpack-plugin": "^4.3.1"
	}
}
